#!/usr/bin/env bash
set -euo pipefail

ROOT_DIR="$(cd "$(dirname "$0")" && pwd)"
OUT_DIR="$ROOT_DIR/examples"

mkdir -p "$OUT_DIR"

# Run the O2 vs O3 demo to generate fresh outputs under demo/output
"$ROOT_DIR/demo/run_compare_O2_vs_O3.sh"

# Copy the produced JSON files into examples/
cp -f "$ROOT_DIR/demo/output/baseline_O2.json" "$OUT_DIR/baseline.json"
cp -f "$ROOT_DIR/demo/output/current_O3_native.json" "$OUT_DIR/current.json"

echo "Examples updated in $OUT_DIR:"
ls -l "$OUT_DIR"/*.json


